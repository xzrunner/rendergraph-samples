                    "frag": "#version 330 core\\nout vec4 FragColor;\\n\\nin VS_OUT {\\n    float height;\\n    vec2 texcoord;\\n} fs_in;\\n\\n\\nuniform sampler2D u_detailmap;\\n\\nuniform sampler2D u_splatmap0;\\nuniform sampler2D u_splatmap1;\\nuniform sampler2D u_splatmap2;\\nuniform sampler2D u_splatmap3;\\n\\nvoid main()\\n{\\n\\tvec4 splat_col;\\n\\tif (fs_in.height < 0.33) {\\n\\t\\tfloat t = fs_in.height / 0.33;\\n\\t\\tsplat_col = texture2D(u_splatmap0, fs_in.texcoord) * (1 - t) + texture2D(u_splatmap1, fs_in.texcoord) * t;\\n\\t} else if (fs_in.height < 0.66) {\\n\\t\\tfloat t = (fs_in.height - 0.33) / 0.33;\\n\\t\\tsplat_col = texture2D(u_splatmap1, fs_in.texcoord) * (1 - t) + texture2D(u_splatmap2, fs_in.texcoord) * t;\\n\\t} else {\\n\\t\\tfloat t = (fs_in.height - 0.66) / 0.33;\\n\\t\\tsplat_col = texture2D(u_splatmap2, fs_in.texcoord) * (1 - t) + texture2D(u_splatmap3, fs_in.texcoord) * t;\\n\\t}\\n\\n\\tvec4 detail_col = texture2D(u_detailmap, fs_in.texcoord);\\n\\tFragColor = splat_col * detail_col;\\n}"
